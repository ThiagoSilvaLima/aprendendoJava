package list;

import java.util.ArrayList;
import java.util.List;

public class program {
	public static void main(String[]args) {
		List<employees> employ = new ArrayList<>();
		employ.add(new employees(772,"Bob Green",5000.00));
		employ.add(new employees(536,"Alex Grey",3000.00));
		employ.add(new employees(333," Maria Brown", 4000.00));
		//program
		
		System.out.println("How many employees will be registred? "+employ.size());
	
		for(int i=0; i < employ.size();i++) {
	
			employ.get(0).getClass();
		}

	}
	
	
}
